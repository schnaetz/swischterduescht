{
    "name": "Swischterduest",
    "version": "1.0.0",
    "description": "This extension will replace texts on the websites you visit and make them more D A N K",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "manifest_version": 2
}